{
    "version": 2,
    "builds": [
        {
            "src": "server/api.js",
            "use": "@vercel/node"
        },
        {
            "src": "client/package.json",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "server/api.js"
        },
        {
            "src": "/(.*)",
            "dest": "client/dist/index.html"
        }
    ]
}